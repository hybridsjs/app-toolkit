{
  "name": "@hybrids/app-tools",
  "version": "0.0.0",
  "description": "...",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "author": "Dominik Luba≈Ñski <dominik.lubanski@gmail.com>",
  "repository": "https://github.com/hybridsjs/app-tools",
  "scripts": {
    "dev": "karma start",
    "dev:coverage": "rm -rf ./coverage && NODE_ENV=coverage npm run dev",
    "test": "karma start --single-run",
    "test:coverage": "rm -rf ./coverage && NODE_ENV=coverage npm run test -- --browsers ChromeHeadless",
    "coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "build": "npm run build:cjs && npm run build:esm && npm run build:dist",
    "build:cjs": "rm -rf ./lib && NODE_ENV=cjs babel ./src -d ./lib -s inline",
    "build:esm": "rm -rf ./esm && babel ./src -d ./esm -s inline",
    "build:dist": "rm -rf ./dist && webpack",
    "release": "npm run test && npm run build && standard-version",
    "example": "cd ./example && parcel ./src/index.html"
  },
  "keywords": [
    "router",
    "stack",
    "layout",
    "flexbox",
    "web components",
    "custom elements",
    "shadow dom"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-3": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "standard-version": "^4.4.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "hybrids": "^1.3.1",
    "path-to-regexp": "^2.2.1",
    "query-string": "^5.1.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "all"
        }
      ],
      "no-param-reassign": 0,
      "no-new-func": 0,
      "no-loop-func": 0,
      "new-cap": 0,
      "prefer-destructuring": 0,
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "test/**/*.js"
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-3"
    ],
    "env": {
      "cjs": {
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ]
        ]
      },
      "coverage": {
        "plugins": [
          [
            "istanbul",
            {
              "include": [
                "src/**/*.js"
              ]
            }
          ]
        ]
      }
    }
  }
}
